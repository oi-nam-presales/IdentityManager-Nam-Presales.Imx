{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "dpr": ["dist/dpr"],
      "dpr/*": ["dist/dpr/*"],
      "qbm": ["dist/qbm"],
      "qbm/*": ["dist/qbm/*"],
      "qer": ["dist/qer"],
      "qer/*": ["dist/qer/*"],
      "aob": ["dist/aob"],
      "aob/*": ["dist/aob/*"],
      "tsb": ["dist/tsb"],
      "tsb/*": ["dist/tsb/*"],
      "att": ["dist/att"],
      "att/*": ["dist/att/*"],
      "rps": ["dist/rps"],
      "rps/*": ["dist/rps/*"],
      "o3t": ["dist/o3t"],
      "o3t/*": ["dist/o3t/*"],
      "aad": ["dist/aad"],
      "aad/*": ["dist/aad/*"],
      "sample-plugin": ["dist/sample-plugin"],
      "sample-plugin/*": ["dist/sample-plugin/*"]
    }
  }
}
